<div class="log-in">
<p>If you're already registered, log in and get started!</p>

<h1>Log in</h1>

<%= form_tag new_login_path do %>
  <p>
    <%= label_tag :email %><br>
    <%= email_field_tag :email %>
  </p>

  <p>
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
  </p>

  <p>
    <%= submit_tag "Log In", class: "button" %>
  </p>
<% end %>

<%= link_to "Register", new_user_path %>
</div>


